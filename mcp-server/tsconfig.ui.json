{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "outDir": "ui/lib",
    "rootDir": "ui/lib",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": false
  },
  "include": ["ui/lib/*.ts"],
  "exclude": ["node_modules"]
}
